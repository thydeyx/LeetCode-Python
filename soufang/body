	<script>
	function pageJump(pagevalue)
	{
		var totalPage = 100;
		var re = /^[0-9]*[1-9][0-9]*$/;
		if(re.test(pagevalue))
		{
			if(parseInt(pagevalue) > parseInt(totalPage))
			{
				pagevalue = parseInt(totalPage);
			}
			var fgoto = "/house/s/b9%5B%3Anum%3A%5D/";
			var fgoto = fgoto.replace("%5B%3Anum%3A%5D",pagevalue);
			window.location.href = fgoto;
		
		}
		else
		{
			alert("请输入大于0的数字");    
		}
	}
	</script>
		
