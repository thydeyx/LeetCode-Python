		 $("a[name='order_price']").attr("id","sjina_C01_33_02");
		}

		
	　　 break;
　　 case 'pa':
		$("a[name='order_default']").removeClass("cur");
		$("a[name='order_price']").addClass("cur");
		$("a[name='order_price']").find('span').css('color','#ff3333');
		$("a[name='order_price']").find("i").addClass("arrow_up_red");
		$("a[name='order_price']").attr("href","/house/s/b1pd/");
