function loadScript(B,D){var A=document.createElement("script"),C=document.documentElement.firstChild;A.type="text/javascript";if(A.readyState){A.onreadystatechange=function()                        {if(A.readyState=="loaded"||A.readyState=="complete"){A.onreadystatechange=null;D()}}}else{A.onload=function(){D()}}A.src=B;C.insertBefore(A,C.firstChild)};
</script>

<script language="javascript">
		var domainStr = "";
		var newcodes = "";
		var projNames = "";
		var obj = document.getElementById('houseHistory');
		function giveDomainValue(strDomain) {
			var newcodesArr = newcodes.split(",");
			var strDomainArr = strDomain.split(",");
			var projNamesArr = projNames.split(",");
			num = 0;
			for(var i=0; i<newcodesArr.length; i++) {
				if(newcodesArr[i] && strDomainArr[i] && projNamesArr[i]) {
					if(num == 0 || num == 2 || num == 4) {
						elTR = obj.insertRow( -1 );
					}
					elTD = elTR.insertCell( -1 );
					elTD.innerHTML = '<td style="width:50%; font-size:12px;"><a href="'+strDomainArr[i]+'" target="_blank">'+ projNamesArr[i] +'</a></td>';
					num = num + 1;
				}
			}
			if(num > 0) {
				document.getElementById('houseHistory').style.display = '';
			}
		}
		//document.getElementById('form_newhouse').action='/house/web/Search_Result.php';
</script>


