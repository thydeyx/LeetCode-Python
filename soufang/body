<script language="javascript">

function LocationHrefReplace(url)
{
    if(navigator.userAgent.indexOf("MSIE") > 0){
        var referLink = document.createElement('a');
        referLink.href = url; 
        document.body.appendChild(referLink);
        referLink.click(); 
    }else{
         window.location.replace(url); 
    }
}
function hidedis(k)
{	
	$("#p_"+k).css("display","none");
	$("#huise_"+k).attr("class","clearfix");
}
function showdis(k)
{	
	$("#p_"+k).css("display","");
	$("#huise_"+k).attr("class","clearfix huise");
}
function orderImgShow(){

	$("a[name='order_price']").find('span').text("价格");
	$("a[name='order_sale']").find('span').text("开盘时间");
	$("a[name='order_dpcount']").find('span').text("点评");
	
	$("a[name='order_price']").find("i").removeAttr("class");
	$("a[name='order_sale']").find("i").removeAttr("class");
	$("a[name='order_dpcount']").find("i").removeClass("arrow_down_red");

	 $("a[name='order_price']").removeClass("cur");
	 $("a[name='order_sale']").removeClass("cur");
	 $("a[name='order_dpcount']").removeClass("cur");
	 $("a[name='order_default']").removeClass("cur");
　　switch (orderStr)
　　 {
　　 case 'pd':
		$("a[name='order_default']").removeClass("cur"); 
		$("a[name='order_price']").addClass("cur");  
		$("a[name='order_price']").find('span').css('color','#ff3333');
		$("a[name='order_price']").find("i").addClass("arrow_down_red");
		$("a[name='order_price']").attr("href","/house/s/b1pa/");
		if(sType){
		 $("a[name='order_price']").attr("id","sjina_C01_41_02");
