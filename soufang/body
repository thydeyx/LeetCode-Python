function getN9District(num)
{
    var returnString = ""; var getNum = 0;
    for(var i=0; i<20; i++)
    {
        var string = _ub.n9;
        if(typeof(string) != 'undefined')
        {
            _ub.load(0);
            var stringArr = string.split("^");
            if(stringArr[2] == '1')
            {
                returnString += stringArr[0]+"|";
                getNum++;
                if(getNum == num)
                {
                    return returnString;
                }
            }
        }
    }
    return returnString;
}
function padLeft(str,len)
{
    str = str.toString();
    if(str.length >= len)
        return str;
    else
        return padLeft("0" +str,len);
} 
function callback_displayMaylike()
{
    var getData = this.responseText;
    if(getData){
		 var obj = eval('(' + getData + ')');
		 var htmlContent = "";
		 var j=0;
		 var strnewcode_p = '';
		 for(var i=0; i<obj.length; i++){
			if(i<4){
				j=i+1;
				strnewcode_p = strnewcode_p+','+obj[i]['newcode']+'^'+j+'^'+obj[i]['frompageid'];
				var liContent = '';
				liContent +='<li>';
				liContent +='<div>';
				
				liContent +='<a rel="nofollow" style="" class="infohref" href="'+obj[i]['imgLink']+"_"+j+'" target="_blank">';
				liContent +='<img src="'+obj[i]['picture_thumb']+'" alt="" id="" width="190" height="127">';
				if(obj[i]['pcadnewSign']){
					liContent +='<img src="http://imgd2.soufunimg.com/2015/12/25/25k/71ea846c543c4065b86e95b5b3244712.png" style="position:absolute; top:0px; right:0px; ">';
				}
				liContent +='<span style="width:auto;left: 50%;top: 50px;position: absolute;height: 24px;text-align: center;line-height: 24px;color: #fff;font-weight: bold;transform: translate(-50%,0);margin: 0 auto;opacity: 1;background-color: rgba(0,0,0,0.6);padding: 0 10px;border-radius: 4px;">'+obj[i]['projtype']+'</span>';
				liContent +='</a>';
				
				liContent +='</div>';
				
				liContent +='<p class="clearfix">';
				liContent +='<strong class="fl" style="white-space:nowrap;overflow:hidden;text-overflow:ellipsis;width:100%;"><a href="'+obj[i]['houseurl']+"_"+j+'" target="_blank" id="" title="'+obj[i]['district']+'-'+obj[i]['title']+'">'+obj[i]['district']+'-'+obj[i]['title']+'</a></strong>';

				if(obj[i]['price_unit'] == '元/平方米' || !obj[i]['price_unit']){
					var price_unit = '元/O';
				}else{
					var price_unit = obj[i]['price_unit'];
				}
				if(obj[i]['price_num']){
					liContent +='<p><span style="font-weight: bold;">均价：</span><em>'+obj[i]['price_num']+price_unit+'</em>';
				}else{
					liContent +='<p><span style="font-weight: bold;">均价：</span><em>价格待定</em>';
				}
				
				if(obj[i]['squares']){
					liContent +='<i style="font-style: normal;color: #999;float: right;">'+obj[i]['squares']+'O</i>';
				}		
				liContent +='</p>';
				if(obj[i]['louPanYHInfoTag']){
					liContent +='<p style="width: 100%;padding-top: 5px;"><em style="color: #333;float: left;height: 22px;line-height: 22px;padding:0 10px;border:1px solid #dbdbdb;">'+obj[i]['louPanYHInfoTag']+'</em></p>';
				}
				
				
				liContent +='</p>';
				
				
				
				liContent +='</li>';
				htmlContent += liContent;
			}
		 }
		 trnewcode_p = strnewcode_p.substr(1,10000);
		_ub.collect(1,{'vwn.guesslikeid':strnewcode_p});
		document.getElementById('divMaylikeUl').innerHTML = htmlContent;
        
	}
    document.getElementById('divMaylike').style.display = '';
	//document.getElementById('mayIntrest').style.display = '';
}
var maylikeTop = document.getElementById('divMaylikePosition').offsetTop;
window.onscroll = function () {
    var scrollPos = getScrollingPosition();
    if(scrollPos[1] + window.screen.height > maylikeTop && beginUbRequest == false)
    {
        beginUbRequest = true;
        initUbRequest();
    }
};
