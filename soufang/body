<script type="text/javascript" language="javascript">
    function  download_new()
    {
        var content=document.getElementById("tese");
        var down_btn=document.getElementById("s_btn");
        if(content.style.display=="none")
        {
            content.style.display="block"
            down_btn.className='down';
        }
        else 
        {
            content.style.display="none"
            down_btn.className='up';
        }
    }
    function searchTips(input, tips){
        var input = document.getElementById(input);
        if(!input) return;
        if(input.value==""){
            input.value = tips;
            input.className = "sec_input input_height search-tips";
        }
        input.onblur = function(){
            if(!this.value){
                this.value = tips;
            }
            input.className = "sec_input input_height search-tips";
        }
        input.onmouseover = function(){
            if(this.value == tips){
                this.value = "";
                this.focus();
            }
            input.className = "sec_input0 input_height";
        }
        input.onfocus = function(){
            if(this.value == tips){
                this.value = "";
            }
            input.className = "sec_input0 input_height";
        }
    }
    function sortData(value){
        var fgoto = "/house/s/b1%3F/";
        var fgoto = fgoto.replace("?",value);
        window.location.href = fgoto;
    }
    function sortStartdate(value){
        var fgoto = "/house/s/a4%3F/";
        var fgoto = fgoto.replace("?",value);
        window.location.href = fgoto;
    }
    function sortFit(value){
        var fgoto = "/house/s/c1%3F/";
        var fgoto = fgoto.replace("?",value);
        window.location.href = fgoto;
    }
    function byb_new(url){
        var inputs = document.getElementsByTagName("input");
        var num = 0;

        for(var i = 0; i < inputs.length; i++){
            if(inputs[i].type == "checkbox"){
                if(inputs[i].checked == true){
                    url += inputs[i].value + '|';
                    num++;
                }
            }
        }
        if(num < 2 || num > 4){
            alert('请选择2-4个楼盘进行比较！');
        }else{
            window.open(url);
        }
    }				
</script>
