		<script>
		<!--学区房模块添加丢失的css-->
		$(function(){
			var t='';
			$(".fangyuan").addClass('pr');
			$(".scSerch").each(function(){
			  var _this = $(this);
			  _this.find("em").hover(function() {
			  clearTimeout(t);
			  var pw = _this.parents(".fangyuan").find(".scContanner");
			  pw.slideDown(300);
			 }, function() {
			  t = setTimeout(function(){_this.parents(".clearfix").find(".scContanner").slideUp(300);},200);
			 });
			});
			$(".scContanner").each(function(){
			 var _this=$(this);
			 _this.hover(function() {
			  clearTimeout(t);
