<script language="javascript">
$(function(){
    commonSwitchTab("box_a", "box_ai", "qston","qstoff", "hover");
	commonSwitchTab("box_e01", "box_ei01", "s1","s2", "hover");
	commonSwitchTab("js_lclf", "js_lclfi", "current","", "hover");
	commonSwitchTab("js_lcct", "js_lccti", "current","", "hover");
	commonSwitchTab("js_lcri", "js_lcrii", "current","", "hover");
	commonSwitchTab("cbt", "con_cbt", "hover","", "hover");
});
</script>
<script language="javascript">
function commonSwitchTab(tabId, contentId, onClass, offClass,onHandle) {
	if(onHandle == 'hover') {
		$("[id^='"+tabId+"_']").each(function(index, dom) {
			var idNum = dom.id.replace(tabId+"_", "");
			$(this).hover(function(){
				$("[id^='"+tabId+"_']").each(function(index_, dom_) {
					var idNum_ = dom_.id.replace(tabId+"_", "");
					if(idNum == idNum_) {
						$("#"+dom_.id).addClass(onClass);
						if(offClass){
						    $("#"+dom_.id).removeClass(offClass);
						}
						$("#"+contentId+"_"+idNum_).show();
					} else {
						$("#"+dom_.id).removeClass(onClass);
						if(offClass){
						    $("#"+dom_.id).addClass(offClass);
						}						
						$("#"+contentId+"_"+idNum_).hide();
					}
				});
			}, function(){
			});
		});
	} else if(onHandle == 'click') {
		$("[id^='"+tabId+"_']").each(function(index, dom) {
			$(this).css("cursor", "pointer");
			var idNum = dom.id.replace(tabId+"_", "");
			$(this).click(function(){
				$("[id^='"+tabId+"_']").each(function(index_, dom_) {
					var idNum_ = dom_.id.replace(tabId+"_", "");
					if(idNum == idNum_) {
						$("#"+dom_.id).addClass(onClass);
						if(offClass){
						    $("#"+dom_.id).removeClass(offClass);
						}						
						$("#"+contentId+"_"+idNum_).show();
					} else {
						$("#"+dom_.id).removeClass(onClass);
						if(offClass){
						    $("#"+dom_.id).addClass(offClass);
						}									
						$("#"+contentId+"_"+idNum_).hide();
					}
				});
			});
		});
	}
}
</script>
<!--区域查询 js end---->
