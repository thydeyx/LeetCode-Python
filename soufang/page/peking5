<script text="javasript">
	<!-- 提交订阅 -->
	var isLoginForDy = 0;
	var telephones = '';
	var uid = '';
	var newcode =  "1010833405";
	var authcodeDomain = "http://www.fang.com";
	var dpOneLink = "";
	var currNewcode = '1010833405';
	var city = '北京';
	var projname = '学府锦园';
	var yzm = 'yzm';
	var knotices = '';
	var pnotices = '';
	var znotices = '';
	function submits(){		
		if($('.ml').hasClass('lpt_icon')){
			var jjtz = $(".ml").val();
			knotices = 'k';
		}else{
			knotices = '';
		}
		if($('.mlf').hasClass('lpt_icon')){
			var kptz = $(".mlf").val();
			pnotices = 'p';
		}else{
			pnotices = '';
		}
		if($('.mlz').hasClass('lpt_icon')){
			var zxdt = $(".mlz").val();
			znotices = 'z';
		}else{
			znotices = '';
		}
		if(knotices == '' && pnotices == '' && znotices == ''){
			alert('请选择通知类型');
			return false;
		}
		if(isLoginForDy == 0){
			//需要验证验证码(成功直接调订阅接口)
			var ajaxUrl = "/house/ajaxrequest/house_subscribe.php";
			var phone = $('#dy_immitEnlistTel').val();
			var code = $('#tel_code').val();
			$.ajax({
				type:'get',
				dataType:'jsonp',
				data:{mobilephone:phone,service:"newhouse-open-web",mobilecode:code},
				url:"https://passport.fang.com/loginverifysms.api",
				error:function(){
					alert("验证码错误，请重试");},
				success:function(msg){
					if(msg.Message == "Success" || isLoginForDy == 1){
						$.ajax({
							type:'get',
							dataType:'json',
							url:ajaxUrl,
							data:{mobile:phone, type: 10,authcode: code,city: city,jjtz:knotices, kptz: pnotices,zxdt:znotices,newcode:currNewcode,types:yzm,projname:projname},
							complete:function(){},
							success:function(msg){
								if(msg.result == 103) {
									alert("手机验证码填写错误！");
									$(".code").val('');
									$('.p2').children('.ti').html("请输入验证码");
									return false;
									//huodongValCodeHintaa();
								}else if(msg.code == 1) {
									_ub.city = "北京"; 
									_ub.biz = 'n'; 
									_ub.location = 0; //方位 ，网通为0，电信为1，如果无法获取方位，记录0
									_ub.collect(74,{"vwg.page":"nhpage","vwn.projectid":currNewcode,"vwn.phone":phone});
									$('.success').css('display','block');
									$('#tip').html('恭喜您，已订阅成功！');
									setTimeout(function(){
										$('.success').css('display','none');document.location.reload();
										},2000);
								}else if(msg.code == 2){
									$('.success').css('display','block');
									$('#tip').html('您已经订阅过该楼盘了！');
									setTimeout(function(){
										$('.success').css('display','none');document.location.reload();
										},2000);
								}else{
									$('#tip').html('由于网络原因添加失败请稍后再试!');
									$('.success').css('display','block');
									setTimeout(function(){
										$('.success').css('display','none');document.location.reload();
										},2000);
								}
								
							}
						});
					}else{
						alert(msg.Tip);
						return false;
					}
				}
			});
			
		}
	}
	//点击降价通知 开盘通知 最新动态js
	function chooseType(type){
		if(type == 1)
		{
			if($('.ml').hasClass('lpt_icon')){
				$('.ml').removeClass('lpt_icon');
			}else{
				$('.ml').addClass('lpt_icon');
			}
			
		}
		if(type == 2)
		{	
			if($('.mlf').hasClass('lpt_icon')){
				$('.mlf').removeClass('lpt_icon');
			}else{
				$('.mlf').addClass('lpt_icon');
			}
		
		}
		if(type == 3)
		{	
			if($('.mlz').hasClass('lpt_icon')){
				$('.mlz').removeClass('lpt_icon');
			}else{
				$('.mlz').addClass('lpt_icon');
			}
		}
	}
	
	 $("#dy_immitEnlistTel").bind('focus', function(){
        if($(this).val() == "请输入接收订阅的手机号") {
            $(this).val("");
        }
		$("#dy_immitEnlistTel").css("color","#999999");
    }).bind('blur', function(){
        if($(this).val() == "") {
            $(this).val("请输入接收订阅的手机号");
			$("#dy_immitEnlistTel").css("color","#999999");
        }		
    });
	
	$("#tel_code").bind('focus', function(){
        if($(this).val() == "输入验证码") {
            $(this).val("");
        }
		$("#tel_code").css("color","#999999");
    }).bind('blur', function(){
        if($(this).val() == "") {
            $(this).val("输入验证码");
			$("#tel_code").css("color","#999999");
        }		
    });
	//获取订阅人数
	$.ajax({
		type:'get',
		dataType:'json',
		url:'/house/ajaxrequest/dingyueNum_get.php',
		data:{newcode:currNewcode},
		complete:function(){},
		success:function(msg){
			if(msg.Result == '100'){
				var dingyuesum = parseInt(msg.content[0].SingleCount)+parseInt(msg.content[1].SingleCount);
				$('#dingyueNum').html('已有'+dingyuesum+'人订阅');
			}
		}
	});
</script>

