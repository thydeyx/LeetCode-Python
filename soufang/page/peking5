<script>
	var picidstr = '';
	$.ajax({
		type:'get',
		dataType:'json',
		url:'/house/ajaxrequest/huxingNum_get.php',
		data:{newcode:newcode,city:city,picidstr:picidstr},
		complete:function(){},
		error:function(){},
		success:function(msg){
			if(msg.count){
				for(var i=0;i<msg.count;i++){
					if(msg.data[i].saling == 0){
						$('#saling_'+msg.data[i].picid).html('该户型已抢完');
						$('#btn_'+msg.data[i].picid+' .btn01').css('background','#ddd');
					}else{
						$('#saling_'+msg.data[i].picid).html('仅剩<i>'+msg.data[i].saling+'</i>套');
					}
				}
			}
		}
	});
</script>

