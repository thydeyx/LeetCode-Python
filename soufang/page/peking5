<script>
	// 调整层位置
	var formWidth = parseInt($("#qdds_form").css("width").replace("px", ""));
	var resWidth = parseInt($("#qdds_res").css("width").replace("px", ""));
	$("#qdds_form").css("top", "100px").css("margin-left", (0-formWidth)/2);
	$("#qdds_res").css("top", "100px").css("margin-left", (0-resWidth)/2);
