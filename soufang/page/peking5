<script type="text/javascript" src="http://newhouse.js.soufunimg.com/newhouse/js/jsold/house/detail_new/js/Root.Xml.js"></script>


<script language="javascript">
function getScrollingPosition( ) {
    var position = [0,0];
    if ( typeof window.pageYOffset != 'undefined') {
       position = [ window.pageXOffset, window.pageYOffset];
    } else if ( typeof document.documentElement.scrollTop != 'undefined' && document.documentElement.scrollTop > 0) {
          position = [ document.documentElement.scrollLeft,document.documentElement.scrollTop];                       
    } else if (typeof document.body.scrollTop != 'undefined') {
          position = [ document.body.scrollLeft , document.body.scrollTop];
    }
    return position;
}
var beginUbRequest = false;
var citySuo = "bj";
var featurelp = "";
if(featurelp){
	var interestNum = 4;
}else{
	var interestNum = 1;
}
function initUbRequest()
{
	_ub.request('vwn.key,vwn.position,vwn.subway,vwn.genre,vwn.unitprice,vwn.feature,vwn.salestatus,vwn.housetype,vwn.loopline,vwn.opentime', function(){
			_ub.load(3);//取时间最近的数据
			var ubKeyStr = _ub['vwn.key'].v+';'+_ub['vwn.key'].d+';'+_ub['vwn.key'].m;
			var ubPositionStr = _ub['vwn.position'].v+';'+_ub['vwn.position'].d+';'+_ub['vwn.position'].m;
			var ubSubwayStr = _ub['vwn.subway'].v+';'+_ub['vwn.subway'].d+';'+_ub['vwn.subway'].m;
			var ubGenreStr = _ub['vwn.genre'].v+';'+_ub['vwn.genre'].d+';'+_ub['vwn.genre'].m;
			var ubUnitpriceStr = _ub['vwn.unitprice'].v+';'+_ub['vwn.unitprice'].d+';'+_ub['vwn.unitprice'].m;
			var ubFeatureStr = _ub['vwn.feature'].v+';'+_ub['vwn.feature'].d+';'+_ub['vwn.feature'].m;		
			var ubSalestatusStr = _ub['vwn.salestatus'].v+';'+_ub['vwn.salestatus'].d+';'+_ub['vwn.salestatus'].m;
			var ubHousetypeStr = _ub['vwn.housetype'].v+';'+_ub['vwn.housetype'].d+';'+_ub['vwn.housetype'].m;
			var ubLooplineStr = _ub['vwn.loopline'].v+';'+_ub['vwn.loopline'].d+';'+_ub['vwn.loopline'].m;
			var ubOpentimeStr = _ub['vwn.opentime'].v+';'+_ub['vwn.opentime'].d+';'+_ub['vwn.opentime'].m;
			var ubAllCondition = ubKeyStr+'|'+ubPositionStr+'|'+ubSubwayStr+'|'+ubGenreStr+'|'+ubUnitpriceStr+'|'+ubFeatureStr+'|'+ubSalestatusStr+'|'+ubHousetypeStr+'|'+ubLooplineStr+'|'+ubOpentimeStr;
        
			var requestUrl = "/house/web/ubRequest.php?num="+interestNum+"&frompage=xfxq&from=xfxq&condition="+ubAllCondition+"&newcode="+newcode+"&time=" + new Date().getTime();
			Xml.Request(requestUrl, null, callback_displayMaylike, null);
			/*var openUrl1 = requestUrl+"&DEBUG=1";
			window.open(openUrl1,'_black')*/
		});
		
    _ub.request('n');
}
function getN9District(num)
{
    var returnString = ""; var getNum = 0;
    for(var i=0; i<20; i++)
    {
        var string = _ub.n9;
        if(typeof(string) != 'undefined')
        {
            _ub.load(0);
            var stringArr = string.split("^");
            if(stringArr[2] == '1')
            {
                returnString += stringArr[0]+"|";
                getNum++;
                if(getNum == num)
                {
                    return returnString;
                }
            }
        }
    }
    return returnString;
}

function callback_displayMaylike()
{
    var getData = this.responseText;
	//alert(getData);
    if(getData)
    {
        var obj = eval('(' + getData + ')');
        var htmlContent = "";
		var j=0;
		var strnewcode_p = '';
        for(var i=0; i<obj.length; i++)
        {
        	j=i+1;
        	
        	strnewcode_p = strnewcode_p+','+obj[i]['newcode']+'^'+j+'^'+obj[i]['frompageid'];
            var liContent = '';
			liContent +='<li>';
            liContent +='<div class="pic" id="display_xianshi_'+i+'" onmouseover="display1(\''+i+'\')" onmouseout="display2(\''+i+'\')">';
			liContent +='<a href="'+obj[i]['imgLink']+'"><img class="imgts" src="'+obj[i]['picture_thumb']+'"/><span id="projType_'+i+'" style="display:none">'+obj[i]['projtype']+'</span></a></div>';
			
			if(obj[i]['price_unit'] == '元/平方米' || !obj[i]['price_unit']){
					var price_unit = '元/O';
			}else{
				var price_unit = obj[i]['price_unit'];
			}
			
			if(obj[i]['price_num'] && obj[i]['price_num'] != 0 && obj[i]['price_num'] != '待定'){
				liContent +='<div class="text"><p><a href="'+obj[i]['houseurl']+'">'+obj[i]['district']+'-'+obj[i]['title']+'</a></p><p>均价：<i>'+obj[i]['price_num']+price_unit+'</i></p></div>';
			}else{
				liContent +='<div class="text"><p><a href="'+obj[i]['houseurl']+'">'+obj[i]['district']+'-'+obj[i]['title']+'</a></p><p>均价：<i>价格待定</i></p></div>';
			}
			liContent +='</li>';
			htmlContent += liContent;
        }
        
        strnewcode_p = strnewcode_p.substr(1,10000);
        _ub.collect(1,{'vwn.guesslikeid':strnewcode_p});
        document.getElementById('divMaylikeUl').innerHTML = htmlContent;
		
    }
   // document.getElementById('js_jrtabi_0').style.display = '';
	//document.getElementById('js_jrtab_0').style.display = '';
}
function display1(id){
		$('#projType_'+id).show();
}

function display2(id){
	$('#projType_'+id).hide();
}


window.onscroll = function () {
    var scrollPos = getScrollingPosition();
    if(scrollPos[1] + window.screen.height > parseInt($("#divMaylikePosition").offset().top) && beginUbRequest == false)
    {
        beginUbRequest = true;
        initUbRequest();
    }
	
};
</script>

