<script>
var Feature = (function() {
	var default_id = 0;
	var current_id = 0;
	var count = $("img[id^=feature_]").size();
		// prev
	var movePrev = function() {
		
		current_id = current_id - 1;
		// if no.1 return false;
		if(current_id < 0) {
			++ current_id;
			return false;
		}
		clearAll();
		$("#feature_"+current_id).show();
		activePoint(current_id);
	}
	// next
	var moveNext = function() {
		
		current_id = current_id + 1;
		// if the last one  return false;
		if(current_id >= count) {
			-- current_id;
			return false;
		}
		clearAll();
		$("#feature_"+current_id).show();
		activePoint(current_id);
	}
	
	// active point
	var activePoint = function(id) {
		$("#feature_points").find('li').removeClass('active');
		
		$("#feature_points").find('li').each(function(){
			if('point_'+id == $(this).attr("id")) {
				$('#point_'+id).addClass('active');
			}
		});
	
	}
	
	var movePointer = function(index){
		clearAll();
		$("#feature_"+index).show();
		current_id = index;
		activePoint(index);
	}
	
	var clearAll = function() {
		$("img[id^=feature_]").hide();
	}
	
	var init = (function(){
		clearAll();
		$("#feature_"+default_id).show();
		$('#point_'+default_id).addClass('active');
	}())
	
	return {
		movePrev:movePrev,
		moveNext:moveNext,
		movePointer:movePointer
	}
}())
</script>
    <!--楼盘特色 end-->
	</div>
</div>
