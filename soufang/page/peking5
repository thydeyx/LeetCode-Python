// 立即领取动作绑定
var lingqu = function(){
	qddsData4Newcode = msgRes;
	clearTimeout(sto);
	if (msgRes.isSoufunTuan == 1) {
		qddstype = 'huodong';
		$("#huodong_form").show();
		qddsLayerShowHide();
		initTuanForm();
	} else {
		qddstype = 'qdds';
		//判断是否登录
		if(msgRes.info.islogin)//已登录
		{						
			if(msgRes.bindinfo.type == 3 )//已登录已报名未绑定
			{
				qddsSmsCf = msgRes.bindinfo.OrderIdres;
				$("#qdds_form").show();
				$("#qdds_form_bmpage").hide();
				$("#qdds_form_sucss").show();
				$("#qdds_ordermanager").show();
				$("#qdds_form_zygw").hide();
				initQddsUserForm();
				qddsLayerShowHide();
				initQddsForm();
			}
			else if(msgRes.bindinfo.type == 4)//已登录已报名已绑定
			{
				qddsSmsCf = msgRes.bindinfo.OrderIdres;
				$("#qdds_form").show();
				$("#qdds_form_bmpage").hide();
				$("#qdds_form_sucss").show();
				$("#qdds_ordermanager").show();
				$("#qdds_form_zygw").show();
				initQddsUserForm();
				qddsLayerShowHide();
				initQddsForm();
			}
			else
			{	
				$("#qdds_form").show();
				$("#qdds_form_bmpage").show();
				$("#qdds_form_sucss").hide();
				$("#qdds_ordermanager").hide();
				$("#qdds_form_zygw").hide();
				qddsLayerShowHide();
				initQddsForm();							
			}
		}
		else
		{	
			$("#qdds_form").show();
			$("#qdds_form_bmpage").show();
			$("#qdds_form_sucss").hide();
			$("#qdds_ordermanager").hide();
			qddsLayerShowHide();
			initQddsForm();												
		}
	}
}
